{
  "name": "medreport-generator",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.23",
    "@types/node": "12.12.8",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.2.0",
    "typescript": "3.7.2"
  },
  "scripts": {
    "start": "env-cmd -f ./.env.development.local react-scripts start",
    "build": "react-scripts build",
    "build:local": "env-cmd -f ./.env.production.local npm run build",
    "test": "react-scripts test",
    "predeploy": "env-cmd -f ./.env.production.local npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx src",
    "eslint:dump": "eslint --print-config ./.eslintrc.json",
    "precommit": "lint-staged"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.14.148",
    "@types/semantic-ui": "^2.2.7",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "docx-merger": "^1.2.2",
    "docxtemplater": "^3.14.10",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.0.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "file-saver": "^2.0.2",
    "firebase": "^7.4.0",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.9",
    "jszip": "^3.2.2",
    "jszip-utils": "^0.1.0",
    "lint-staged": "^9.4.3",
    "moment": "^2.24.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "react-select": "^3.0.8",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.1"
  },
  "lint-staged": {
    "*.{ts, tsx}": [
      "prettier --write",
      "git add"
    ]
  }
}
